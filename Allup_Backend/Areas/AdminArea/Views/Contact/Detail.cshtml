@model Contact
@{
    ViewData["Title"] = "Detail";
}

<div class="container">
    <div class="row">
        <div class="col-6">
            <p>@Model.Address</p>
            <p>@Model.EmailAddress</p>
            <p>@Model.PhoneNumber</p>
            <p>@Model.Description</p>
            <p>@Model.MapUrl</p>
            <p>@Model.OpenCloseClocks</p>
        </div>

        <div class="col-6">
            <a asp-controller="Contact" asp-action="Index" class="btn btn-warning" style="color:white">Go Back</a>
        </div>
    </div>
</div>
